import React from 'react';
import { View, Image, Text } from 'react-native';
import styles from './styles';

const ImageThumbnail = function ({ file, name }) {
  return (
    <Image
    style={styles.image}
    resizeMode="cover"
    source={{ uri: file }}
    />
	<Text> {name} </Text>
  );
};

export default ImageThumbnail;
